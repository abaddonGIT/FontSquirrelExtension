{
  "manifest_version": 2,
  "name": "FontSquirrelExtension",
  "description": "This extension used fontsquirrel for searching and download fonts.",
  "version": "1.3.1",
  "icons": {
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "author": "Abaddon",
  "browser_action": {
    "default_icon": "img/icon_48.png",
    "default_title": "FontSquirrel",
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "notifications",
    "downloads",
    "http://www.fontsquirrel.com/*",
    "http://everythingfonts.com/*",
    "http://www.font2web.com/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}